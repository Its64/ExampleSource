*{box-sizing:border-box}body,html{overflow-x:hidden}body{font-family:"Poppins",sans-serif;margin:0;padding:0;font-weight:400;color:#fff;background-color:#191919;font-size:calc(12px + .625vw)!important}@media (max-width:767px){body{font-size:calc(12px + 13.6*(100vw - 320px)/1280)!important}}button{font-size:calc(8px + .546875vw)!important}@media (max-width:767px){button{font-size:calc(8px + 11.9*(100vw - 320px)/1280)!important}}.container{max-width:1000px;margin:0 auto;padding:0 10px}@media (min-width:1919px){.container{max-width:1280px}}@media (min-width:3839px){.container{max-width:1920px}}h1,h2,h3,h4,h5,h6,p{margin:0;padding:0}h1{font-size:calc(24px + 1.25vw)!important}@media (max-width:767px){h1{font-size:calc(24px + 27.2*(100vw - 320px)/1280)!important}}h2{font-size:calc(21.6px + 1.125vw)!important}@media (max-width:767px){h2{font-size:calc(21.6px + 24.48*(100vw - 320px)/1280)!important}}h3{font-size:calc(16.8px + .875vw)!important}@media (max-width:767px){h3{font-size:calc(16.8px + 19.04*(100vw - 320px)/1280)!important}}h4{font-size:calc(12px + .625vw)!important}@media (max-width:767px){h4{font-size:calc(12px + 13.6*(100vw - 320px)/1280)!important}}ul,ul li{padding:0;margin:0}ul{display:flex}ul li{list-style:none}::selection{background:#000!important;color:#fff!important;opacity:1!important;-webkit-text-fill-color:normal!important;text-fill-color:normal!important}.white{color:#fff!important}.black{color:#000!important}.class{opacity:1}.header{position:fixed;width:100%;padding-bottom:2px!important;background-color:rgba(255,255,255,.075)}.header .container ul{padding:0;margin:0;display:flex;justify-content:center;align-items:center;align-self:center}.header .container ul li{transition:.15s ease;padding:calc(12px + .625vw);font-size:calc(12px + .625vw);background-color:transparent;line-height:1.15}.header .container ul li:hover{border-bottom:2px solid #fffc87;cursor:pointer;background-color:rgba(0,0,0,.15)}.header .container ul li a{text-decoration:none;color:#fff;opacity:1;font-weight:650;letter-spacing:.07em}main{margin-top:calc((24px + 1.40625vw)*3.5)}.hero .container,.hero .container .btn-div{display:flex;text-align:center;justify-content:center}.hero .container{flex-direction:column}.hero .container h1{font-size:calc((24px + 1.40625vw)*1.75)!important;background:linear-gradient(90deg,#fffc87 0,#ff8787 100%);background-clip:text;text-fill-color:transparent;-webkit-background-clip:text;-webkit-text-fill-color:transparent;line-height:1.1}.hero .container .btn-div a,.hero .container p{font-weight:600;font-size:calc((12px + .625vw)*1.25)}.hero .container p{z-index:-.1;letter-spacing:.05em;opacity:.7;position:relative;bottom:1px;padding-bottom:calc(12px + .625vw)}.hero .container .btn-div{width:calc((24px + 1.40625vw)*5.5)!important;background:linear-gradient(90deg,rgba(255,252,135,.9) 0,rgba(255,135,135,.9) 100%);padding:calc(12px + .625vw) calc((12px + .625vw)*1.75);align-items:center;margin:0 auto}.hero .container .btn-div a{text-decoration:none;color:#fff}.hero .container .btn-div:hover{cursor:pointer}